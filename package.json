{
  "name": "react-rss-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "rollup-plugin",
    "vite-plugin"
  ],
  "scripts": {
    "dev": "ts-node-esm server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --e2e",
    "cypress:report": "npx nyc report && npx nyc report --reporter=text-summary",
    "cypress:e2e": "npx start-server-and-test dev http-get://localhost:3001 cypress:run",
    "test:cypress": "npm run cypress:e2e && npm run cypress:report"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/node": "^18.16.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.3.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint-config-prettier": "^8.7.0",
    "express": "^4.18.2",
    "h3": "^1.6.4",
    "i": "^0.3.7",
    "laravel-vite-plugin": "^0.7.4",
    "listhen": "^1.0.4",
    "msw": "^1.2.1",
    "normalize.css": "^8.0.1",
    "npm": "^9.6.5",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sirv": "^2.0.2",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.2.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "cypress": "^12.11.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jsdom": "^21.1.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.61.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.29.8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
